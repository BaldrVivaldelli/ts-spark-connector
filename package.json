{
  "name": "ts-spark-connector",
  "version": "1.2.0",
  "description": "A TypeScript client for Apache Spark Connect. Build Spark logical plans entirely in TypeScript.",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "prepack": "npm run build",
    "prepare": "npm run build",
    "release": "semantic-release"
  },
  "keywords": [
    "spark",
    "spark-connect",
    "typescript",
    "dataframe",
    "bigdata",
    "dsl",
    "apache",
    "grpc"
  ],
  "author": "Augusto Vivaldelli",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/BaldrVivaldelli/ts-spark-connector"
  },
  "bugs": {
    "url": "https://github.com/BaldrVivaldelli/ts-spark-connector/issues"
  },
  "homepage": "https://github.com/BaldrVivaldelli/ts-spark-connector#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.33.0",
    "rimraf": "^6.0.0",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "apache-arrow": "^21.0.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "scope": "api",
              "release": "minor"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "release",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": false
            },
            {
              "type": "test",
              "release": false
            },
            {
              "type": "chore",
              "release": false
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          }
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": []
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ]
    ]
  }
}
